<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminManageClubsActivity">

    <Button
        android:id="@+id/createClubsButton"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="Create Club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.108" />

    <Button
        android:id="@+id/seeClubsButton"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="See Clubs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.108" />

    <Button
        android:id="@+id/updateClubsButton"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="Update Clubs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/createClubsButton"
        app:layout_constraintVertical_bias="0.14" />

    <Button
        android:id="@+id/deleteClubsButton"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="Delete Club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seeClubsButton"
        app:layout_constraintVertical_bias="0.141" />

    <Button
        android:id="@+id/clubRequestsButton"
        android:layout_width="151dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:text="Club Requests"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/createSubClubsButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/updateClubsButton"
        app:layout_constraintVertical_bias="0.144" />

    <Button
        android:id="@+id/createSubClubsButton"
        android:layout_width="146dp"
        android:layout_height="58dp"
        android:text="Create Sub-Club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deleteClubsButton"
        app:layout_constraintVertical_bias="0.137" />

    <Button
        android:id="@+id/banUserButton"
        android:layout_width="147dp"
        android:layout_height="57dp"
        android:layout_marginTop="48dp"
        android:text="BAN USER"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clubRequestsButton"
        app:layout_constraintVertical_bias="0.085" />

    <Button
        android:id="@+id/unbanUserButton"
        android:layout_width="147dp"
        android:layout_height="57dp"
        android:layout_marginTop="48dp"
        android:text="unban user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/createSubClubsButton"
        app:layout_constraintVertical_bias="0.085" />
</androidx.constraintlayout.widget.ConstraintLayout>