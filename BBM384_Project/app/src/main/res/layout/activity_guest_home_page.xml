<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GuestHomePage">

    <Button
        android:id="@+id/browseSubClubsButton"
        android:layout_width="210dp"
        android:layout_height="105dp"
        android:layout_marginBottom="140dp"
        android:text="Browse Sub Clubs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/browseClubsButton"
        app:layout_constraintVertical_bias="0.729" />

    <Button
        android:id="@+id/browseClubsButton"
        android:layout_width="210dp"
        android:layout_height="105dp"
        android:layout_marginTop="248dp"
        android:text="Browse Clubs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>